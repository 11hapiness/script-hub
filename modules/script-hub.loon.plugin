[Script]

http-request ^https?:\/\/script\.hub\/$ script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/script-hub.js, tag=Script-hub

http-request ^(https?:\/\/)?script\.hub.+&type=qx-rewrite&target=loon-plugin script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/QX-Rewrite-Parser.js, requires-body=true, tag=QX转换Loon

http-request ^(https?:\/\/)?script\.hub.+&type=loon-plugin&target=loon-plugin script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/Loon-Rewrite-Parser.js, requires-body=true, tag=Loon插件控制器

http-request ^(https?:\/\/)?script\.hub.+&type=surge-module&target=loon-plugin script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/Surge-Rewrite-Parser.js, requires-body=true, tag=Surge转换Loon

http-request ^(https?:\/\/)?script\.hub.+&type=[^&]+&target=rule-set script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/rule-parser.js, requires-body=true, tag=规则集转换

http-request _script-converter-(stash|surge|loon|shadowrocket)\.js$ script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/script-converter/script-converter.js, requires-body=true, tag=脚本转换1

http-request _script-converter-(STASH|SURGE|LOON|SHADOWROCKET)\.js$ script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/script-converter/script-converter.js, requires-body=true, tag=脚本转换2, argument="wrap_response=true"

[MITM]

hostname = github.com,raw.githubusercontent.com,gitlab.com,gist.githubusercontent.com,script.hub
